<h1>Create a review to <%= @restaurant.name %> </h1>

<%= simple_form_for([@restaurant, @review]) do |f| %>
     <%= f.input :content %>
     <%= f.input :rating %>
  <%= f.submit "add a review" %>
<% end %>

<div class="links">
  <%= link_to "Back to the restaurant", restaurant_path(@restaurant) %>
</div>
